{
  "name": "Medium Focus",
  "description": "Make Medium great again by removing distracting elements",
  "version": "0.0.0.4",
  "manifest_version": 3,

  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content-script.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  }
}
